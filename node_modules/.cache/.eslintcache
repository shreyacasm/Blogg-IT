   \n    \n    \n    \n    <h1 className=\"home-head\">View Other Articles</h1>\n    <ArticleList articles={otherArticles}/>\n    </div>\n   ); \n    \n}\nexport default ArticlePage;","/home/hp/Desktop/reactprojects/link-course-blog/client/src/pages/ArticleListPage.js",["75"],"import React from 'react';\nimport ArticleList from '../Components/ArticlesList';\nimport articleContent from './article-content';\nimport {ReadOutlined } from '@ant-design/icons';\nimport ArticleImg from './article.jpg';\nconst ArticleListPage = () => (\n    <>\n    <div className=\"article-page\">\n        <h1 className=\"home-head\" style={{textAlign:\"center\", fontSize:\"50px\", paddingTop:\"0px\"}}>Articles</h1>\n        <ReadOutlined className=\"read-icon ic2\" />\n        <ArticleList articles={articleContent}/>\n    </div>\n    </>\n);\nexport default ArticleListPage;","/home/hp/Desktop/reactprojects/link-course-blog/client/src/pages/NotFoundPage.js",[],"/home/hp/Desktop/reactprojects/link-course-blog/client/src/pages/article-content.js",[],"/home/hp/Desktop/reactprojects/link-course-blog/client/src/Components/AddComment.js",[],"/home/hp/Desktop/reactprojects/link-course-blog/client/src/Components/CommentsList.js",["76"],"import React from 'react';\nimport './article.css';\nimport {UserOutlined } from '@ant-design/icons';\n\nconst CommentList = ({ comments }) => (\n    <div className=\"comment-div\">\n    <h3><b>Comments :</b></h3>\n    <hr className=\"comment-hr\"/>\n    {comments.map((comment, key) =>(\n        <div key={key}>\n            <h4><b>{comment.username}</b></h4>\n            <p>{comment.text}</p>\n        </div>\n    ))}\n    <hr className=\"comment-hr\"/>\n    </div>\n);\nexport default CommentList;","/home/hp/Desktop/reactprojects/link-course-blog/client/src/Components/ArticlesList.js",["77","78"],"import React from 'react';\nimport {Link} from 'react-router-dom';\nimport \"./article.css\";\nimport 'antd/dist/antd.css';\nimport { Row, Col, Slider } from 'antd';\nimport